package maliha_p3;

	import java.util.Scanner;
	public class maliha_p3 {

	public static void main(String[] args) 
	{

			//prompt to rate each topic
			Scanner scnr = new Scanner(System.in);
			
			//topics
			String[] topics = {"Life", "Politics", "School", "GLobal Warming", "Food"};
			
			//ratings
				
			int [][] ratings = new int [topics.length][];
			int m, n;
			
			int numUsers = 0;
			int rating = 0;
				
				
			for (m = 0; m < rating; m++)
			{
				ratings[m] = new int[10];
				for (n = 0; n < ratings[m].length; n++);
				ratings[m][n] = 0;
			}
				
			// get number of users
				
			System.out.print("How many users will be rating?");
			numUsers = scnr.nextInt();
				
			//for loop
			System.out.println("Please rate the following topics from 1(least important) to 10 (most important)");
				
			for (n = 0; n < topics.length; n++)
			{
				System.out.print("Enter desired rating for " + topics[n] + ": ");
				rating = scnr.nextInt();
			}
		
				System.out.print("Thank you! Program ends :)");
				
			}
		}
